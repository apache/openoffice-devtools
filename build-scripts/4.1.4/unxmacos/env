MACOSX_DEPLOYMENT_TARGET=10.7
ANT_HOME=/usr/local/share/java/apache-ant
ANT_CLASSPATH=/usr/local/share/java/apache-ant/lib
PATH=~/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/X11/bin:.
